{
  "name": "s3-upload-api",
  "version": "1.0.0",
  "description": "API to get URI's for uploading files",
  "repository": {
    "type": "git",
    "url": "https://github.com/javelinco/s3_upload"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "precommit-message": "echo Running pre-commit checks... && exit 0",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write --loglevel error \"src/**/*.ts\" && git add .",
    "compile": "rimraf dist/ && tsc -p ."
  },
  "pre-commit": [
    "precommit-message",
    "lint",
    "format",
    "compile"
  ],
  "author": "Mark Munson",
  "dependencies": {
    "@hapi/boom": "^7.4.11",
    "@hapi/hapi": "^19.0.5",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.0",
    "@hapi/vision": "^6.0.0",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.1",
    "aws-sdk": "^2.612.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "hapi-decorators": "^2.0.0",
    "hapi-swagger": "^12.0.0",
    "moment": "^2.24.0",
    "pretty-bytes": "^5.3.0",
    "systeminformation": "^4.19.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/hapi__boom": "^7.4.1",
    "@types/hapi__hapi": "^18.2.6",
    "@types/node": "^12.12.12",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  }
}
